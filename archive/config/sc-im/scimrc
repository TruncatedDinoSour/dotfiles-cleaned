color "type=HEADINGS fg=WHITE bg=BLACK bold=0"
color "type=HEADINGS_ODD fg=WHITE bg=BLACK bold=0"

color "type=CELL_SELECTION fg=WHITE bg=RED bold=1"
color "type=CELL_SELECTION_SC fg=WHITE bg=RED bold=1"


nnoremap "#" "G"
nnoremap "^" "gg"
nnoremap "G" "#"
nnoremap "#" "G"

nnoremap ";" ":"
vnoremap ";" ":"

nnoremap "fj" "f-"
nnoremap "fk" "f+"

